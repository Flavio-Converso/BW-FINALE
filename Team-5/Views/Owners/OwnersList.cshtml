@model IEnumerable<Team_5.Models.Clinic.Owners>

@{
    ViewData["Title"] = "Owners List";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Phone Number</th>
            <th>CF</th>
            <th>User</th>
          
        </tr>
    </thead>
    <tbody>
        @foreach (var owner in Model)
        {
            <tr>
                <td>@owner.Name</td>
                <td>@owner.Surname</td>
                <td>@owner.PhoneNumber</td>
                <td>@owner.CF</td>
                <td>@owner.User?.ToString() ?? "N/A"</td> <!-- Adjust based on how you want to display User info -->
               
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="CreateOwner" class="btn btn-success">Create New Owner</a>
</div>
